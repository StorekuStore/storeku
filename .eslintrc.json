{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "new-cap": 0,
        "no-unused-vars": 0,
        "import/order": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "no-restricted-syntax": 0
    }
}